<Window x:Class="SDPFReportGenerator.OxyPlotGraph"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf" 



    Title="OxyPlotGraph" Height="300" Width="300">
    <Grid>
        <oxy:Plot>
            <oxy:LineSeries ItemsSource="{Binding Path=xp}" DataFieldX="planNo" DataFieldY="cpuUsage" />
        </oxy:Plot>
    </Grid>
</Window>
